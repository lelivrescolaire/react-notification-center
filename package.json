{
  "name": "@lls/react-notification-center",
  "version": "1.2.5",
  "description": "react-notification-center keep all your notification in a single place",
  "main": "lib/index.js",
  "scripts": {
    "start": "better-npm-run dev-server",
    "build:w": "better-npm-run build:w",
    "build": "better-npm-run build && npm run less && npm run less:m",
    "less": "node_modules/.bin/lessc src/less/index.less --autoprefix=\"last 2 versions\" lib/css/react-redux-notification.css",
    "less:m": "node_modules/.bin/lessc src/less/index.less --autoprefix=\"last 2 versions\" --clean-css lib/css/react-redux-notification.min.css",
    "clean": "better-npm-run clean",
    "lint": "npx lls-scripts lint",
    "buildc": "better-npm-run build_client",
    "lintOld": "better-npm-run lint",
    "format": "npx lls-scripts format"
  },
  "eslintConfig": {
    "root": true
  },
  "betterScripts": {
    "dev-server": {
      "command": "./node_modules/.bin/webpack-dev-server --hot --inline --config build/webpack.config.js",
      "env": {
        "NODE_ENV": "development",
        "NODE_PORT": 4001
      }
    },
    "build_client": {
      "command": "npx webpack-cli -p --progress --config build/webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "npx babel src/ --out-dir lib"
    },
    "build:w": {
      "command": "npx babel -w src/ --out-dir lib"
    },
    "lint": {
      "command": "npx eslint . --ext .js --ext .jsx || true"
    },
    "clean": {
      "command": "npx rimraf dist lib"
    }
  },
  "author": "Diego Oliveira",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lelivrescolaire/react-notification-center"
  },
  "bugs": {
    "url": "https://github.com/diegoddox/react-notification-center/issues"
  },
  "files": [
    "lib/",
    "CHANGELOG.md",
    "README.md"
  ],
  "keywords": [
    "React.js",
    "React",
    "react",
    "notification",
    "react-notification",
    "react-component",
    "notification",
    "react-notification",
    "react notifications",
    "react notification",
    "changelog"
  ],
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@lls/babel": "7.11.4",
    "@lls/lls-scripts": "0.13.4",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "better-npm-run": "0.1.1",
    "chance": "^1.0.1",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-react": "7.18.3",
    "html-webpack-plugin": "3.2.0",
    "less": "3.10.3",
    "less-loader": "5.0.0",
    "less-plugin-autoprefix": "2.0.0",
    "less-plugin-clean-css": "^1.5.1",
    "lorem-ipsum": "2.0.3",
    "moment": "^2.12.0",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-hot-loader": "4.12.19",
    "rimraf": "3.0.1",
    "style-loader": "1.1.3",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.2"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "element-class": "^0.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
